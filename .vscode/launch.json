{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug run_ui.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/run_ui.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["-Xfrozen_modules=off", "--port=8695", "--dockerized=true"],
      "python": "${command:python.interpreterPath}",
      "cwd": "${workspaceFolder}",
      "env": {"PYTHONPATH": "${workspaceFolder}", "RFC_PASSWORD": "1123"}
    },
    {
      "name": "Debug current file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": ["-Xfrozen_modules=off", "--dockerized=true"],
      "python": "${command:python.interpreterPath}",
      "cwd": "${workspaceFolder}",
      "env": {"PYTHONPATH": "${workspaceFolder}", "RFC_PASSWORD": "1123"}
    }
  ]
}
