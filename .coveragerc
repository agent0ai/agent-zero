# Coverage.py configuration for Agent Zero

[run]
source = .
omit =
    */tests/*
    */test_*.py
    *_test.py
    */venv/*
    */.venv/*
    */dist/*
    */build/*
    */webui/vendor/*
    */docker/*
    */tmp/*
    */logs/*
    setup.py

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    class .*\bProtocol\):
    @overload

ignore_errors = True

[html]
directory = htmlcov
